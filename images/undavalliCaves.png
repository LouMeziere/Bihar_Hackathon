                    {{/if}}
                    {{#if Sunrise}}
                    <p>Sunrise: <span class="sunrise"> {{Sunrise}} </span>
                        Sunset: <span class="sunset"> {{Sunset}} </span>
                    </p>
                    {{/if}}
                </div>
            </div>
            <div class="flex-column">
                <div class="explore-template--humid">
                    <img src="/etc.clientlibs/incredible-india/clientlibs/clientlib-site/resources/images/HumidityIcon.svg"
                         alt="Humidity Icon"/>
                    {{#if Humidity}}
                    <p>Humidity : <span class="humidity">{{Humidity}}</span>%</p>
                    {{/if}}
                </div>

                <div class="explore-template--wind">
                    <img src="/etc.clientlibs/incredible-india/clientlibs/clientlib-site/resources/images/Wind.svg"
