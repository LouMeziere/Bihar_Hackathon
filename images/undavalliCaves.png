        <div class="images">
            <img src="{{cityThumbnail}}" class="glance-img" alt="City">

            <div class="image-text">
                {{#if cityTitle}}
                <h3>{{cityTitle}}</h3>
                {{/if}}
                {{#if cityDescription}}
                <p>
                    {{cityDescription}}
                </p>
                {{/if}}
                {{#if cityURL}}
                <a href="{{cityURL}}" class="inc-btn__small">
                    Explore
                </a>
                {{/if}}
            </div>
        </div>
    </div>
