<script id="exploreCityTemplateWeather" type="text/x-handlebars-template">
    <div class="explore-template__weathertoday">
        <p>Weather today</p>
        <div class="explore-template--info">
            <div class="flex-row">
                <div class="explore-template--temp">
                    <!-- <img src="/content/dam/incredible-india/images/global/weather/{{{[Weather Code]}}}.{{WeatherType}}" alt="Sunrise Icon">   -->
                    <img src="/content/dam/incredible-india/images/global/weather/3.svg" alt="Sunrise Icon">
                    {{#if Temperature}}
